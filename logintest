
const url = 'url';
const username = 'USERNAME';
const password = 'PASSWORD';

const start = Date.now();

try {
    // Step 1 — Open login page
    browser.open(url);
    browser.await(() => browser.page().includes('Login'), 5000);

    // Step 2 — Enter credentials and submit (adjust element selectors as needed)
    browser.element('input[name="userNameInput"]').setValue(username);
    browser.element('input[name="passwordInput"]').setValue(password);
    browser.element('button[type="submit"]').click();

    // Step 3 — Wait for Overview page
    browser.await(() => browser.page().includes('Overview'), 10000);

    const elapsed = (Date.now() - start) / 1000.0;

    return JSON.stringify({
        status: 'OK',
        response_time: elapsed,
        message: 'Login OK (Overview found)'
    });

} catch (error) {
    const elapsed = (Date.now() - start) / 1000.0;
    return JSON.stringify({
        status: 'FAIL',
        response_time: elapsed,
        message: `Login failed or error: ${error.message}`
    });
}
